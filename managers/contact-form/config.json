{
  "name": "contact-form",
  "title": "Contact Form",
  "description": "Allows users to submit a web form in order to ask questions, leave feedback, and communicate with you directly.",
  "version": 1,
  "category": "Forms",
  "author": "Solodev",
  "authorWebsite": "https://www.solodev.com/",
  "projectURL": "https://github.com/solodev/marketplace/managers/contact-form/",
  "featuredImage": "featured-image.jpg",
  "entryPoint": {
    "path": "/<!manager_name!>",
    "type": "Datatable"
  },
  "dependencies": {
    "solodevCMS": "8.9"
  },
  "Asset_File": [{
    "name": "careers-form.tpl",
    "from_path": "/_Assets/contact-form.tpl",
    "to_path": "/_Assets/contact-form.tpl"
  },
	{
    "name": "contact-us-thanks.tpl",
    "from_path": "/_Assets/contact-thanks.tpl",
    "to_path": "/_Assets/contact-thanks.tpl"
  }],
  "Datatable": [{
    "name": "<!manager_name!>",
    "module_type": "Datatable",
    "datatable_type": "Form",
    "to_path": "/<!manager_name!>",
    "icon_type": "fab fa-wpforms",
    "form_template": {
      "file_path": "/_Assets/contact-form.tpl"
    },
		"return_template": {
				"file_path": "/_Assets/contact-thanks.tpl"
		},
    "entries": []
  }],
  "Asset_Category": [{
    "from_path": "/_Assets",
    "to_path": "/_Assets"
  }]
}